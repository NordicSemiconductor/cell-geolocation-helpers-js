{
  "name": "@bifravst/cell-geolocation-helpers",
  "version": "0.0.0-development",
  "description": "Helper functions for the cell geolocation feature",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "start": "webpack-dev-server --config-name development --open",
    "build": "npx webpack --config-name production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/cell-geolocation-helpers.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/cell-geolocation-helpers/issues"
  },
  "homepage": "https://github.com/bifravst/cell-geolocation-helpers",
  "keywords": [
    "Cloud",
    "IoT",
    "Bifravst",
    "Asset Tracker",
    "NordicSemiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@bifravst/code-style": "8.0.134",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "@types/jest": "26.0.14",
    "@types/leaflet": "1.5.19",
    "@types/node": "14.11.8",
    "@types/react": "16.9.52",
    "@types/react-dom": "16.9.8",
    "@types/react-leaflet": "2.5.2",
    "@types/styled-components": "5.1.4",
    "husky": "4.3.0",
    "jest": "26.5.3",
    "lint-staged": "10.4.0",
    "nmea-simple": "3.2.0",
    "prettier": "2.1.2",
    "source-map-loader": "1.1.1",
    "styled-components": "5.2.0",
    "ts-jest": "26.4.1",
    "ts-loader": "8.0.4",
    "typescript": "4.0.3",
    "webpack": "5.0.0",
    "webpack-cli": "4.0.0",
    "webpack-dev-server": "3.11.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npx jest --onlyChanged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "jest": {
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".+\\.spec\\.ts$",
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  },
  "dependencies": {
    "@turf/turf": "^5.1.6",
    "fp-ts": "2.8.4"
  }
}
