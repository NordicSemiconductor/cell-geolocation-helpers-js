{
  "name": "@bifravst/cell-geolocation-helpers",
  "version": "0.0.0-development",
  "description": "Helper functions for the cell geolocation feature",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "start": "webpack-dev-server --config-name development --open",
    "build": "npx webpack --config-name production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/cell-geolocation-helpers.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/cell-geolocation-helpers/issues"
  },
  "homepage": "https://github.com/bifravst/cell-geolocation-helpers",
  "keywords": [
    "Cloud",
    "IoT",
    "bifravst",
    "AssetTracker",
    "NordicSemiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@bifravst/code-style": "^6.0.5",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@types/jest": "^25.1.4",
    "@types/leaflet": "^1.5.10",
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-leaflet": "^2.5.1",
    "@types/styled-components": "^5.0.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "source-map-loader": "^0.2.4",
    "styled-components": "^5.0.1",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged && npx jest --onlyChanged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "package-lock.json",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "jest": {
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".+\\.spec\\.ts$",
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  },
  "dependencies": {
    "fp-ts": "^2.5.3"
  }
}
