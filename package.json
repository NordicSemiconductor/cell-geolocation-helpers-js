{
  "name": "@nordicsemiconductor/cell-geolocation-helpers",
  "version": "0.0.0-development",
  "description": "Helper functions for the cell geolocation feature",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "start": "snowpack dev",
    "build": "snowpack build",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/cell-geolocation-helpers-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/cell-geolocation-helpers-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/cell-geolocation-helpers-js",
  "keywords": [
    "nrf-asset-tracker",
    "iot"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.79",
    "@prefresh/snowpack": "3.1.4",
    "@snowpack/plugin-babel": "2.1.7",
    "@snowpack/plugin-react-refresh": "2.5.0",
    "@snowpack/plugin-sass": "1.4.0",
    "@snowpack/plugin-typescript": "1.2.1",
    "@swc/jest": "*",
    "@types/jest": "27.5.2",
    "@types/leaflet": "1.7.10",
    "@types/node": "17.0.31",
    "@types/react": "18.0.10",
    "@types/react-dom": "18.0.5",
    "@types/snowpack-env": "2.3.4",
    "@types/styled-components": "5.1.25",
    "jest": "28.1.0",
    "leaflet": "1.8.0",
    "nmea-simple": "3.2.0",
    "normalize.css": "8.0.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "snowpack": "3.8.8",
    "source-map-loader": "3.0.1",
    "styled-components": "5.3.5"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$"
  },
  "dependencies": {
    "@turf/distance": "6.5.0"
  }
}
