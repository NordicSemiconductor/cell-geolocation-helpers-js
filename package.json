{
  "name": "@bifravst/cell-geolocation-helpers",
  "version": "0.0.0-development",
  "description": "Helper functions for the cell geolocation feature",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "start": "snowpack dev",
    "build": "snowpack build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/cell-geolocation-helpers.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/cell-geolocation-helpers/issues"
  },
  "homepage": "https://github.com/bifravst/cell-geolocation-helpers",
  "keywords": [
    "Cloud",
    "IoT",
    "Bifravst",
    "Asset Tracker",
    "NordicSemiconductor"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@bifravst/code-style": "8.0.134",
    "@bifravst/eslint-config-typescript": "5.0.85",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "@prefresh/snowpack": "2.2.0",
    "@snowpack/plugin-babel": "2.1.5",
    "@snowpack/plugin-react-refresh": "2.3.9",
    "@snowpack/plugin-sass": "1.2.0",
    "@snowpack/plugin-typescript": "1.1.1",
    "@types/jest": "26.0.19",
    "@types/leaflet": "1.5.19",
    "@types/node": "14.14.19",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-leaflet": "2.5.2",
    "@types/snowpack-env": "2.3.3",
    "@types/styled-components": "5.1.7",
    "eslint": "7.16.0",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "leaflet": "1.7.1",
    "lint-staged": "10.5.3",
    "nmea-simple": "3.2.0",
    "normalize.css": "8.0.1",
    "prettier": "2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-leaflet": "3.0.5",
    "snowpack": "2.18.5",
    "source-map-loader": "1.1.3",
    "styled-components": "5.2.1",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.13",
    "typescript": "4.1.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && npx jest --onlyChanged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package-lock.json",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "jest": {
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      ".+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".+\\.spec\\.ts$",
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  },
  "dependencies": {
    "@turf/turf": "5.1.6",
    "fp-ts": "2.9.3"
  }
}
